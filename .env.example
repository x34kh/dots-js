# Environment Configuration
# Copy this file to .env and fill in your values

# Google OAuth Client ID (optional - required for Google Sign-In)
GOOGLE_CLIENT_ID=

# Secret key for anonymous user token signing
# Generate a strong random key for production
ANONYMOUS_SECRET=your-secure-random-secret-key-here

# Server port
PORT=8080

# Node environment
NODE_ENV=development

# Redis URL (for production with persistence)
REDIS_URL=redis://localhost:6379
